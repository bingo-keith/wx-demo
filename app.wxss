/**app.wxss**/
.container {
  height: 350rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  background: #09bb07;
  box-sizing: border-box;
} 
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

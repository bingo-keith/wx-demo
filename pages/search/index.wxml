<!--index.wxml-->
<view class="search-container">
    <view wx:for="{{findItems}}">
        <view class="search-container-items" bindtap='test'>
            <view class='search-container-left'>
                <image src="{{item.itemSrc}}" class='search-items-image'></image>
                <text class='search-items-text'>{{item.itemName}}</text>
            </view>
            <text class='search-items-right'>></text>
        </view>
        <progress percent='0' stroke-width='1'></progress>
    </view>
</view>
<canvas canvas-id="myCanvas" style="border: 1px solid;"/>